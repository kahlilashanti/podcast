<div id="podcast_show">
  <div id="show_banner">
    <div class="wrapper_skinny">
      <h1>Seth's Podcast Dashboard</h1>
    </div>
  </div>
  <div id="links">
    <div class="wrapper_skinny">
      <ul id="menu">
        <li class="list_menu"><%= link_to "Episodes", root_path %></li>
        <li class="list_menu"><%= link_to "New Episode", new_podcast_episode_path(@podcast) %></li>
        <li class="list_menu"><%= link_to "Account Settings", edit_podcast_registration_path%></li>
        <li class="list_menu"><%= link_to "View Podcast", podcast_path(current_podcast) %></li>
      </ul>
    </div>
  </div>

  <div id="form">
    <div class="wrapper_skinny">
  <%= render 'form' %>

  </div>
</div>
